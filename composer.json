{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "edvinaskrucas/notification": "5.2.*",
        "zizaco/entrust": "5.2.x-dev",
        "cviebrock/eloquent-sluggable": "^4.1",
        "yajra/laravel-datatables-oracle": "~6.0",
        "barryvdh/laravel-debugbar": "^2.1",
        "laravelcollective/html": "^5.2",
        "baum/baum": "~1.1",
        "intervention/image": "~2.3",
        "intervention/imagecache": "^2.3",
        "felixkiss/uniquewith-validator": "2.*",
        "hisorange/browser-detect": "2.*",
        "nesbot/carbon": "~1.14",
        "barryvdh/laravel-dompdf": "0.8.*",
        "predis/predis": "~1.1@dev",
        "maatwebsite/excel": "~2.1.0",
        "skovmand/mailchimp-laravel": "1.*",
        "elasticquent/elasticquent": "dev-master",
        "mollie/mollie-api-php": "1.8.x",
        "flynsarmy/db-blade-compiler": "2.*",
        "freshheads/postcode-api-client": "^2.0",
        "khill/lavacharts": "~3.0",
        "spatie/laravel-googletagmanager": "^2.2",
        "roumen/sitemap": "^2.6",
        "roumen/feed": "~2.10",
        "socialengine/sniffer-rules": "^2.2",
        "tylercd100/lern": "^3.6",
        "unisharp/laravel-ckeditor": "^4.5",
        "unisharp/laravel-filemanager": "^1.6",
        "chriskonnertz/open-graph": "~1"

    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Hideyo\\Backend\\": "packages/hideyo/backend/src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}